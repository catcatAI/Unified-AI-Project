      "description": "Focus on audio-related models",
      "datasets": ["audio_samples", "common_voice_zh"],
      "epochs": 20,
      "batch_size": 8,
      "target_models": ["audio_service"],
      "use_gpu": false,
      "distributed_training": false
    },
    "math_model_training": {
      "description": "Train mathematical calculation model",
      "datasets": ["arithmetic_train_dataset"],
      "epochs": 50,
      "batch_size": 64,
      "target_models": ["math_model"],
      "checkpoint_interval": 5,
      "use_gpu": false,
      "distributed_training": false
    },
    "logic_model_training": {
      "description": "Train logical reasoning model",
