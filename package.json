{"name": "unified-ai-project", "version": "6.2.0", "description": "Angela AI - Complete AGI System with HSP Protocol and Multi-Agent Architecture", "private": true, "workspaces": ["packages/*", "apps/*"], "scripts": {"dev": "pnpm --filter backend dev && pnpm --filter frontend-dashboard dev", "dev:backend": "cd apps/backend && python -m uvicorn src.services.main_api_server:app --host 127.0.0.1 --port 8000 --reload", "dev:desktop": "cd apps/desktop-app/electron_app && pnpm start", "dev:all": "pnpm dev:backend & pnpm dev:desktop", "test": "pytest tests/", "test:backend": "cd apps/backend && pytest tests/", "test:coverage": "pytest --cov=apps/backend/src --cov-report=html", "lint": "pnpm lint:python && pnpm lint:js", "lint:python": "flake8 apps/backend/src tests/ && mypy apps/backend/src", "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx", "format": "pnpm format:python && pnpm format:js", "format:python": "black apps/backend/src tests/ && isort apps/backend/src tests/", "format:js": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"", "check": "pre-commit run --all-files", "setup": "pip install -r requirements.txt && pnpm install", "build": "pnpm build:desktop", "build:desktop": "cd apps/desktop-app/electron_app && pnpm build", "health-check": "python health_check.py"}, "devDependencies": {"@eslint/eslintrc": "^3.1.0", "@types/node": "^20.11.0", "eslint": "^8.56.0", "eslint-config-next": "^14.1.0", "globals": "^14.0.0", "prettier": "^3.2.5", "typescript": "^5.3.3"}, "engines": {"node": ">=16.0.0", "pnpm": ">=8.0.0", "python": ">=3.8.0"}, "repository": {"type": "git", "url": "https://github.com/catcatAI/Unified-AI-Project"}, "keywords": ["agi", "ai", "hsp", "multi-agent", "live2d", "electron", "python", "fastapi"], "author": "Angela AI Development Team", "license": "MIT"}